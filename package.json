{
  "name": "readfile-directory-index-fallback",
  "version": "1.2.0",
  "description": "fs.readFile using the directory index as a fallback",
  "repository": "shinnn/readfile-directory-index-fallback",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "eslint --config node_modules/@shinnn/eslintrc-node/rc.json index.js test/test.js",
    "test": "node test/test.js | tap-spec",
    "coverage": "istanbul cover --no-default-excludes -x=node_modules/** test/test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/readfile-directory-index-fallback/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "local",
    "read",
    "file",
    "fs",
    "fallback",
    "directory",
    "index",
    "directory-index",
    "asynchronous",
    "buffer"
  ],
  "dependencies": {
    "strip-bom": "^1.0.0"
  },
  "devDependencies": {
    "@shinnn/eslintrc-node": "^1.0.2",
    "eslint": "^0.20.0",
    "istanbul": "^0.3.13",
    "istanbul-coveralls": "^1.0.2",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0"
  }
}
