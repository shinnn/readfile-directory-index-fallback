{
  "name": "readfile-directory-index-fallback",
  "version": "1.2.1",
  "description": "fs.readFile using the directory index as a fallback",
  "repository": "shinnn/readfile-directory-index-fallback",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "pretest": "eslint --config node_modules/@shinnn/eslintrc-node/rc.json index.js test/test.js",
    "test": "node test/test.js | tap-spec",
